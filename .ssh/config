Host *.aci.ics.psu.edu
    ForwardX11Trusted yes
    ForwardAgent yes
    ForwardX11 yes
    ControlMaster auto
    ControlPath ~/.ssh/sockets/%r@%h-%p
    ControlPersist 600
    User suv87
    RequestTTY force
    RemoteCommand /usr/bin/fish

